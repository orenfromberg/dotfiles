Host github.com
    User git
    IdentityFile ~/.ssh/github_id_ed25519

Host gitlab.com
    User git
    IdentityFile ~/.ssh/gitlab_id_ed25519

Host 192.168.1.*
    User bob
    IdentityFile ~/.ssh/localnet_id_ed25519

### Jumpbox configuration.
Host vetsgov-dev-jumpbox-govwest-1b
    HostName ec2-96-127-111-109.us-gov-west-1.compute.amazonaws.com
    User dsva
    ForwardAgent yes
    IdentityFile ~/.ssh/vetsdotgov_id_rsa
Host vetsgov-prod-jumpbox-govwest-1a
    HostName ec2-96-127-111-119.us-gov-west-1.compute.amazonaws.com
    User dsva
    ForwardAgent yes
    IdentityFile ~/.ssh/vetsdotgov_id_rsa
Host vetsgov-prod-jumpbox-govwest-1b
    HostName ec2-96-127-109-160.us-gov-west-1.compute.amazonaws.com
    User dsva
    ForwardAgent yes
    IdentityFile ~/.ssh/vetsdotgov_id_rsa
Host vetsgov-staging-jumpbox-govwest-1b
    HostName ec2-96-127-76-84.us-gov-west-1.compute.amazonaws.com
    User dsva
    ForwardAgent yes
    IdentityFile ~/.ssh/vetsdotgov_id_rsa
Host dsva-vetsgov-utility-jumpbox-1b
  HostName ec2-52-222-45-200.us-gov-west-1.compute.amazonaws.com
  User dsva
  ForwardAgent yes
  IdentityFile ~/.ssh/vetsdotgov_id_rsa
Host dsva-vetsgov-utility-jumpbox-1a
  HostName ec2-52-222-38-230.us-gov-west-1.compute.amazonaws.com
  User dsva
  ForwardAgent yes
  IdentityFile ~/.ssh/vetsdotgov_id_rsa

### Access to SOCKS proxy from public internet, by way of dev jumpbox
Host socks
   HostName 172.31.2.171
   ProxyCommand ssh -l dsva -A 96.127.111.109 -W %h:%p
   User socks
   IdentityFile ~/.ssh/vetsdotgov_id_rsa

### Access to SOCKS proxy from VA network, by way of dev jumpbox
Host socks-va
   HostName 172.31.2.171
   ProxyCommand ssh -l dsva -A 10.238.66.162 -W %h:%p
   User socks
   IdentityFile ~/.ssh/vetsdotgov_id_rsa

### Host aliases that maps CIDR blocks to through their jumpbox bastion.
### Use netcat (nc) as a lightweight ssh proxy.

# Dev network.
Host ip-172-30-0-*.us-gov-west-1.compute.internal
  ProxyCommand  ssh vetsgov-dev-jumpbox-govwest-1b nc -w 120 %h %p
  User dsva
  IdentityFile ~/.ssh/vetsdotgov_id_rsa
